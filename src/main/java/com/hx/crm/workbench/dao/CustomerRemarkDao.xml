<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hx.crm.workbench.dao.CustomerRemarkDao">
    <insert id="save">

        insert into tbl_customer_remark (
            id,
            noteContent,
            createTime,
            createBy,
            editFlag,
            customerId
        )
        values (
                   #{id},
                   #{noteContent},
                   #{createTime},
                   #{createBy},
                   #{editFlag},
                   #{customerId}
               )

    </insert>
	
  	
</mapper>

































